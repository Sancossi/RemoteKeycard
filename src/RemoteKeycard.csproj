<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>RemoteKeycard</AssemblyName>
    <Authors>iRebbok</Authors>
    <Copyright>Copyright iRebbok 2020 - $([System.DateTime]::Now.ToString("yyyy"))</Copyright>
    <TargetFramework>net472</TargetFramework>
    <PlatformTarget>x64</PlatformTarget>

    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <GenerateAssemblyInformationalVersionAttribute>false</GenerateAssemblyInformationalVersionAttribute>
    <GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Assembly-CSharp">
      <HintPath>..\dependencies\Assembly-CSharp.dll</HintPath>
    </Reference>
    <Reference Include="EXILED">
      <HintPath>..\..\Report-INtegration\ServerReport\Libs\EXILED.dll</HintPath>
    </Reference>
    <Reference Include="EXILED_Events">
      <HintPath>..\..\Report-INtegration\ServerReport\Libs\EXILED_Events.dll</HintPath>
    </Reference>
    <Reference Include="Mirror">
      <HintPath>..\dependencies\Mirror.dll</HintPath>
    </Reference>
    <Reference Include="UnityEngine.CoreModule">
      <HintPath>..\dependencies\UnityEngine.CoreModule.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
